{
  "manifest_version": 2,
  "name": "URLCollector",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "google-sheets.png"
  },
  "icons": {
      "16": "google-sheets16.png",
      "32": "google-sheets.png",
      "64": "google-sheets64.png",
      "128": "google-sheets128.png"
    }
}

